<DTS:Property DTS:Name="DisableEventHandlers">0</DTS:Property><DTS:ObjectData><SQLTask:SqlTaskData SQLTask:Connection="{66FE4EAD-D9FE-4736-9CA1-84B7A9AA5D1E}" SQLTask:TimeOut="0" SQLTask:IsStoredProc="False" SQLTask:BypassPrepare="True" SQLTask:SqlStmtSourceType="DirectInput" SQLTask:SqlStatementSource="/***** table creation used to retrieve status of providers who changed from full/part time to per diem *****/&#xA;&#xA;DROP table LawsonPAchanges_items&#xA;;&#xA;&#xA;&#xA;SELECT *&#xA;INTO LawsonPAchanges_items&#xA;FROM&#xA;(&#xA; SELECT&#xA;  hrh.beg_date, pad.item_name, ep.job_code,&#xA;  CASE&#xA;   WHEN pad.data_type = 'A' THEN hrh.A_VALUE&#xA;   WHEN pad.data_type = 'D' THEN CONVERT(varchar,hrh.D_VALUE,101)&#xA;   WHEN pad.data_type = 'N' THEN CAST(hrh.N_VALUE as varchar)&#xA;  END as change_value, &#xA;  CASE&#xA;   WHEN pad.item_name = 'FTE' THEN cast(epo.fte as varchar)&#xA;   WHEN pad.item_name = 'Emp Base Pay Rate' THEN cast(epos.pay_rate as varchar)&#xA;   WHEN pad.item_name = 'Job Code' THEN epo.job_code&#xA;   WHEN pad.item_name = 'Process Level' THEN cast(epo.process_level as varchar)&#xA;  END as prev_value,&#xA;  ep.employee,&#xA;  hrh.pos_level,&#xA;  hrh.date_stamp, &#xA;  replace(convert(varchar,hrh.date_stamp,111),'/','') +&#xA;  CASE &#xA;   WHEN len(CAST(hrh.time_stamp as varchar)) =4  THEN &#xA;    '00' + CAST(hrh.time_stamp as varchar)&#xA;   WHEN len(CAST(hrh.time_stamp as varchar)) =5   THEN &#xA;    '0' + CAST(hrh.time_stamp as varchar)&#xA;   ELSE CAST(hrh.time_stamp as varchar)&#xA;  END as date_time_stamp, &#xA;  hrh.time_stamp,&#xA;  COALESCE(hre.a_field,' ') as rehire_flag&#xA; FROM AppStaging.dbo.LawsonHRHISTORY hrh &#xA;  INNER JOIN AppStaging.dbo.LawsonPADICT pad ON hrh.fld_nbr = pad.fld_nbr &#xA;  INNER JOIN AppStaging.dbo.LawsonPAEMPPOS ep ON ep.employee = hrh.employee &#xA;   AND ep.pos_level = hrh.pos_level&#xA;   --AND ep.date_stamp = hrh.date_stamp&#xA;   AND ep.effect_date = hrh.beg_date&#xA;  LEFT OUTER JOIN AppStaging.dbo.LawsonHREMPUSF hre ON hrh.employee = hre.employee&#xA;   AND hre.field_key = 97&#xA;  LEFT OUTER JOIN AppStaging.dbo.LawsonPAEMPPOS epo ON hrh.employee = epo.employee&#xA;   AND hrh.pos_level = epo.pos_level&#xA;   --AND ep.date_stamp = hrh.date_stamp&#xA;   AND epo.end_date = hrh.beg_date - 1&#xA;  LEFT OUTER JOIN AppStaging.dbo.LawsonPAEMPPOS epos ON ep.employee = epos.employee&#xA;   AND ep.process_level = epos.process_level&#xA;   --AND ep.date_stamp = hrh.date_stamp&#xA;   AND epos.end_date = ep.effect_date - 1&#xA; WHERE  (LEFT(ep.job_code,6)='EPHYAS' OR LEFT(ep.job_code,4)='ERNP'&#xA;   OR LEFT(ep.job_code,6)='EDIRPA' OR LEFT(ep.job_code,8)= 'ELSNPANP' )&#xA; AND hrh.fld_nbr in (14,19,20,24,56,62,899)&#xA; &#xA;) x&#xA;;" SQLTask:CodePage="1252" SQLTask:ResultType="ResultSetType_None" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask"/></DTS:ObjectData></DTS:Executable>
