<property id="4" name="OpenRowsetVariable" dataType="System.String" state="default" isArray="false" description="Specifies the variable that contains the name of the database object used to open a rowset." typeConverter="" UITypeEditor="" containsID="false" expressionType="None"></property>
<property id="5" name="SqlCommand" dataType="System.String" state="default" isArray="false" description="The SQL command to be executed." typeConverter="" UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=10.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91" containsID="false" expressionType="None">DECLARE @v_start_date as datetime
DECLARE @v_end_date as datetime

SET @v_start_date = cast(month(getDate()) as varchar) + '/1/' + cast(year(getDate()) -1 as varchar)
SET @v_end_date = cast(month(getDate()) as varchar) + '/1/' + cast(year(getDate()) as varchar)

/*** Set these values for manual date ranges	***/
--SET @v_start_date='1/1/2012'
--SET @v_end_date='1/1/2013'

BEGIN

/******  Terminated Full Time / Part Time PA data for the period	*****/
SELECT 	CASE
		WHEN e.date_hired + 30 &gt;= e.term_date AND CONVERT(varchar,e.term_date,101) &lt;&gt; '01/01/1753' THEN 'X'
		ELSE ''
	END as flag_30_day,
	a.provider_name as provider_name, a.lawson_id, convert(varchar,a.effective_date,101) as effective_date, 
	a.current_site as site, a.current_site_id as process_level, 
	a.current_position as position, a.current_status, e.emp_status, a.reason_01, a.reason_02,
	convert(varchar,e.date_hired,101) as date_hired,
	convert(varchar,e.term_date,101) as date_termed,
	COALESCE(hre.a_field,' ') as rehire_flag
FROM LawsonPAchanges a 
	INNER JOIN AppStaging.dbo.LawsonEMPLOYEE e ON a.lawson_id = e.employee
	LEFT OUTER JOIN AppStaging.dbo.LawsonHREMPUSF hre ON e.employee = hre.employee
		AND hre.field_key = 97
WHERE 	a.effective_date &gt;= @v_start_date
	AND a.effective_date &lt; @v_end_date
	AND a.current_status IN ('FULL TIME', 'PART TIME')
	AND a.emp_status_code IN ('T0','T1','T2','T4')
	AND a.provider_action LIKE ('%SEPARATE%')
	AND a.pos_level = 1
GROUP BY
	CASE
		WHEN e.date_hired + 30 &gt;= e.term_date AND CONVERT(varchar,e.term_date,101) &lt;&gt; '01/01/1753' THEN 'X'
		ELSE ''
	END,
	a.provider_name, a.lawson_id, convert(varchar,a.effective_date,101), 
	a.current_site, a.current_site_id , 
	a.current_position , a.current_status, e.emp_status, a.reason_01, a.reason_02,
	convert(varchar,e.date_hired,101),
	convert(varchar,e.term_date,101),
	COALESCE(hre.a_field,' ')
	
UNION ALL

SELECT 	CASE
		WHEN e.date_hired + 30 &gt;= e.term_date AND CONVERT(varchar,e.term_date,101) &lt;&gt; '01/01/1753' THEN 'X'
		ELSE ''
	END as flag_30_day,
	ltrim(rtrim(e.first_name)) + ' ' + ltrim(rtrim(e.last_name)) as provider_name,
	a.employee as lawson_id,
	CONVERT(varchar,a.beg_date,101) as effective_date, 
	upper(pr.name) as site, ep.process_level, jc.description as position, 'PER DIEM' as current_status,
	e.emp_status, 
	COALESCE(pa.reason_01,' ') as reason_01, COALESCE(pa.reason_02,' ') as reason_02,
	CONVERT(varchar,e.date_hired,101) as date_hired,
	CASE
		WHEN CONVERT(varchar,e.term_date,101) = '01/01/1753'  THEN ' '
		ELSE CONVERT(varchar,e.term_date,101)
	END as date_termed,
	a.rehire_flag
FROM LawsonPAchanges_items a
	INNER JOIN AppStaging.dbo.LawsonEMPLOYEE e ON e.employee = a.employee
	INNER JOIN AppStaging.dbo.LawsonPAEMPPOS ep ON ep.employee = a.employee
			AND ep.effect_date = a.beg_date 
			AND ep.pos_level = a.pos_level
	RIGHT OUTER JOIN AppStaging.dbo.LawsonPERSACTHST pa ON pa.employee = e.employee
			AND pa.effect_date = e.term_date
			--AND pa.pos_level = a.pos_level
			--AND pa.reason_01 IS NOT NULL
			AND pa.action_code LIKE 'TERM%'
	INNER JOIN AppStaging.dbo.LawsonPRSYSTEM pr ON pr.process_level = ep.process_level
	INNER JOIN AppStaging.dbo.LawsonJOBCODE jc ON jc.job_code = ep.job_code
WHERE cast(a.beg_date as datetime) &gt;= @v_start_date
	and cast(a.beg_date as datetime) &lt; @v_end_date
	and a.item_name = 'FTE'
	and a.change_value = '0.250000'
	and left(a.prev_value,3) IN ('1.0','0.5')
	and a.pos_level='1'
	and (e.emp_status Like 'T%' and e.term_date &lt; @v_end_date)
GROUP BY ltrim(rtrim(e.first_name)) + ' ' + ltrim(rtrim(e.last_name)),
	a.employee,
	a.beg_date, pr.name, ep.process_level, jc.description,
	e.emp_status, 
	COALESCE(pa.reason_01,' ') , COALESCE(pa.reason_02,' '),
	e.date_hired,
	e.term_date,
	a.rehire_flag
order by provider_name
;

END</property>
